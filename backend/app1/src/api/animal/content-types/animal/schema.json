{
  "kind": "collectionType",
  "collectionName": "animals",
  "info": {
    "singularName": "animal",
    "pluralName": "animals",
    "displayName": "Animal",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Nome": {
      "type": "string",
      "unique": true
    },
    "Data_nascimento": {
      "type": "date"
    },
    "Sexo": {
      "type": "enumeration",
      "enum": [
        "Macho",
        "Femea"
      ],
      "required": true
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Liberado para o trabalho",
        "Baixado",
        "Passando por medicacao"
      ],
      "required": true
    },
    "Racao_Diaria": {
      "type": "integer"
    },
    "Idade": {
      "type": "integer",
      "max": 25
    },
    "Racaos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::racao.racao",
      "mappedBy": "Animal"
    },
    "Box": {
      "type": "string"
    },
    "Alocacao": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::unidade.unidade"
    },
    "Condutor_principal": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::condutor.condutor"
    },
    "Condutor_secundario": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::condutor.condutor"
    },
    "Foto": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
